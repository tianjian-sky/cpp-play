  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.defaultFormatter.sass": "sass-formatter",
  "vetur.format.defaultFormatterOptions": {
    "js-beautify-html": {
      "wrap_attributes": "auto"
    },
    "prettier": {
      "singleQuote": true,
      "semi": false,
      "arrowParens": "avoid",
      "htmlWhitespaceSensitivity": "ignore",
      "trailingComma": "none",
      "printWidth": 1000
    }
  },
